<resources>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
        <item name="android:statusBarColor">@android:color/transparent</item>
    </style>

    <style name="RatingBar"
        parent="android:style/Widget.Material.RatingBar.Small">
        <item name="colorControlNormal">@color/endblue</item>
        <item name="colorControlActivated">@color/startblue</item>
    </style>

    <style name="CustomerToolbar" parent="Theme.AppCompat.Light">
        <item name="colorPrimary">#655421</item>
        <item name="colorPrimaryDark">#44391A</item>
        <item name="colorAccent">#808000</item>
        <item name="android:textStyle">bold</item>
        <item name="android:statusBarColor">#44391A</item>
        <item name="android:textColor">#44391A</item>
        <!--<item name="windowActionBar">false</item>-->
        <item name="windowNoTitle">true</item>
    </style>
</resources>
